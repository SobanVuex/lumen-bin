{
    "name": "sobanvuex/lumen-bin",
    "description": "LumenBin - Minimalistic pastebin service.",
    "keywords": ["lumen-bin", "lumen", "bin", "pastebin", "paste", "bin"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "^152.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
